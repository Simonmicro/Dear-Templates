<?xml version="1.0" standalone='no'?>
<!-- Located under /etc/avahi/services/samba.service -->
<!DOCTYPE service-group SYSTEM "avahi-service.dtd">
<service-group>
   <name replace-wildcards="yes">%h</name> # Display Name
   <service>
       <type>_smb._tcp</type>
       <port>445</port>
   </service>
</service-group>
